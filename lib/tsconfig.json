{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": ["ES2017", "es7", "es6", "DOM"],
        "jsx": "react",
        "declaration": true,
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": ["node_modules", "dist"]
}
